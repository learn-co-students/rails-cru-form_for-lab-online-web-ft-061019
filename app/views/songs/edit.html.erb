<%= form_for @song do |f| %> 
    <%= f.label :name, 'Song Name:' %> 
    <%= f.text_field :name %> <br>

    <%= f.label :artist_id, 'Song Artist ID:' %>
    <%= f.text_field :artist_id %> <br>

    <%= f.label :genre_id, 'Song Genre ID:' %> 
    <%= f.text_field :genre_id %> 

    <%= f.submit 'Update Song'     %>
    <% end %> 