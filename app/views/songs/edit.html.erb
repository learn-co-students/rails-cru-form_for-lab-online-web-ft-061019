<h3> Edit Song</h3>

<%= form_for @song do |s| %>
    <%= s.label :name, "Song Name:" %>
    <%= s.text_field :name %><br>
    
    <%= s.label :artist_id, "Artist ID:" %>
    <%= s.text_field :artist_id %><br>

    <%= s.label :genre_id, "Genre ID:" %>
    <%= s.text_field :genre_id %><br>

    <%= s.submit %>
<% end %>